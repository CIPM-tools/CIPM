<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest>
    <application name="MyHTTPClient">
        <crown name="MyHTTPClient">
            <desc>user script</desc>
            <serves>
                <function name="submitData">
                    <desc>Submit data to a hardcoded url</desc>
                    <param name="obj" type="object" multiplicity="1" desc="The data to be submitted"/>
                </function>
            </serves>
        </crown>
        <entry path="scripts" default="main.lua"/>
        <meta key="author">Author</meta>
        <meta key="version">1</meta>
        <meta key="priority">medium</meta>
        <meta key="copy-protected">false</meta>
        <meta key="read-protected">false</meta>
    </application>
</manifest>
